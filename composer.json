{
  "name": "flux-eco/value-object",
  "description": "Offers value objects",
  "version": "1.1.0",
  "type": "flux-app",
  "keywords": [
    "flux-eco",
    "value-object",
    "fluxlabs"
  ],
  "homepage": "https://fluxlabs.ch",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@fluxlabs.ch"
  },
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "ramsey/uuid": ">=4.1"
  },
  "autoload": {
    "files": [
      "fn/getArrayFromMapping.php",
      "fn/getCurrentTime.php",
      "fn/getNewUuid.php",
      "fn/getNewMessage.php",
      "fn/getMessageFromJson.php"
    ],
    "psr-4": {
      "FluxEco\\ValueObject\\": [
        "src/"
      ],
      "fluxValueObject\\": [
        "fn/"
      ]
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform-check": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
